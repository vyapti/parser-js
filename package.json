{
  "name": "@vyapti/parser",
  "version": "0.1.1",
  "author": "Srinivas Dhanwada <dhanwada.dev@gmail.com> (https://github.com/pr1sm)",
  "description": "lcov coverage parser for files and streams",
  "keywords": [
    "coverage",
    "lcov",
    "parser",
    "parsing",
    "file",
    "stream",
    "typescript",
    "umd"
  ],
  "homepage": "https://github.com/vyapti/parser-js/blob/master/README.md",
  "bugs": "https://github.com/vyapti/parser-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vyapti/parser-js.git"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/esm5/index.js",
  "esnext": "dist/esm2015/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.3",
    "@types/sinon": "^7.0.9",
    "@types/sinon-chai": "^3.2.2",
    "@types/whatwg-streams": "^0.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^6.0.3",
    "glob": "^7.1.3",
    "istanbul-lib-coverage": "^2.0.4",
    "istanbul-lib-report": "^2.0.8",
    "istanbul-reports": "^2.2.4",
    "jsdom": "^13.0.0",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "pr1sm/karma-coverage#use_istanbul_lib_packages",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-typescript": "^4.0.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.11",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "scripts": {
    "build": "run-s clean:build build:tsc build:tsc:es2018 build:webpack",
    "build:dev": "run-s clean:build build:tsc",
    "build:dev:watch": "run-s clean:build build:tsc:watch",
    "build:tsc": "tsc",
    "build:tsc:es2018": "tsc --module esnext --target es2018 --outDir dist/esm2015",
    "build:tsc:watch": "tsc -w",
    "build:webpack": "webpack",
    "clean": "run-s clean:build clean:coverage clean:docs",
    "clean:all": "rimraf dist/ coverage/ .nyc_output/ docs/ node_modules/ yarn.lock",
    "clean:build": "rimraf dist/",
    "clean:coverage": "rimraf coverage/ .nyc_output/",
    "clean:docs": "rimraf docs/",
    "coverage": "run-s clean:coverage coverage:node coverage:web coverage:combine",
    "coverage:combine": "node scripts/combineCoverage.js",
    "coverage:node": "nyc --report-dir \"coverage/node\" mocha",
    "coverage:web": "node scripts/runKarma.js",
    "docs": "run-s clean:docs docs:gen",
    "docs:gen": "typedoc --mode file --out docs src/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -t stylish --project \"tsconfig.dev.json\"",
    "lint:fix": "tslint -t stylish --fix --project \"tsconfig.dev.json\"",
    "test": "mocha",
    "test:karma": "karma start",
    "test:watch": "nodemon --watch src --watch test -e ts,js --exec \"clear; yarn run test\""
  }
}
